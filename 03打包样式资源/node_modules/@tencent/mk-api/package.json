{
  "name": "@tencent/mk-api",
  "version": "2.28.0",
  "description": "码客api接口调用封装",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "npm run lint && jest",
    "build": "rimraf lib && tsc -p .",
    "dev": "tsc -w -p .",
    "prepublishOnly": "npm run build",
    "lint": "eslint ./src --ext .ts",
    "typedoc": "typedoc --inputFiles lib --includeDeclarations --excludeExternals --out docs",
    "release": "ts-node --project tsconfig.tsnode.json script/index",
    "release-beta": "npm run release beta",
    "release-test": "npm run release test",
    "release-next": "npm run release next",
    "release-latest": "npm run release latest",
    "release-hotfix": "npm run release hotfix",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "marker"
  ],
  "author": "zwingzheng@tencent.com",
  "license": "ISC",
  "devDependencies": {
    "@tencent/eslint-config-mk": "^0.1.13",
    "@tencent/husky-commit-msg": "^1.0.3",
    "@tencent/husky-eslint-precommit": "^1.0.3",
    "@types/axios": "^0.14.0",
    "@types/jest": "^24.9.0",
    "@types/node": "^14.14.20",
    "@types/qs": "^6.9.5",
    "@types/semver": "^7.3.4",
    "cac": "^6.7.1",
    "chalk": "^4.1.0",
    "conventional-changelog-cli": "^2.1.1",
    "husky": "^4.3.6",
    "jest": "^24.9.0",
    "nock": "^13.0.5",
    "rimraf": "^3.0.2",
    "semver": "^7.3.4",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typedoc": "^0.16.11",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "axios": "^0.19.0"
  },
  "dependencies": {
    "qs": "^6.9.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "node node_modules/@tencent/husky-eslint-precommit/lib/pre-commit.js"
    }
  },
  "config": {
    "unsafe-perm": true
  }
}
